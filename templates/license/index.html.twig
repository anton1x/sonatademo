{% extends 'layout.html.twig' %}

{% block inner %}
    <div id="about_certs_1">
        <div>
            <div class="title">Лицензии</div>
        </div>
    </div>

    <div id="about_certs_2">
        <div>
            <div class="content">
                {% for item in list %}
                <div>
                    <a href="{% path item.image, 'reference' %}" target="_blank">
                        <span class="img"><img src="{% path item.image, 'reference' %}"></span>
                        <span class="title">{{ item.title }}</span>
                    </a>
                </div>
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}

