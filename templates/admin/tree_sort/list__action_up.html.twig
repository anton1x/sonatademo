{% block field %}
    {% spaceless %}
		{% if object.parent is not null and object.parent.left + 1 != object.left %}
		<a class="btn btn-sm btn-default" href="{{ admin.generateObjectUrl('up', object) }}">
            <i class="fa fa-angle-up"></i>
        </a>
		{% endif %}
    {% endspaceless %}
{% endblock %}
